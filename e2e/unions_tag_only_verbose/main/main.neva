import {
    fmt
}

def Main(start any) (stop any) {
    day_printer DayPrinter
    ---
    :start -> Day::Friday -> day_printer -> :stop
}

type Day union {
    Monday
    Tuesday
    Wednesday
    Thursday
    Friday
    Saturday
    Sunday
}

def DayPrinter(day Day) (sig any) {
    println fmt.Println
    ---
    :day -> println:data
    println:res -> :sig
}
