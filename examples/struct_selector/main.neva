import { fmt }

type User struct {
    name string
    pet Pet
}

type Pet struct { name string }

def Main(start any) (stop any) {
    get_user GetUser
    println fmt.Println
    ---
    :start -> get_user -> .pet.name -> println -> :stop
}

const user User = {
    name: 'John',
    pet: { name: 'Charley' }
}

def GetUser(sig any) (u User) {
    :sig -> $user -> :u
}
