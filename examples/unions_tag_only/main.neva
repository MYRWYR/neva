import { fmt }

type Day union {
    Monday
    Tuesday
    Wednesday
    Thursday
    Friday
    Saturday
    Sunday
}

def Main(start any) (stop any) {
    print_day PrintDay
    ---
    :start -> { Day::Friday -> print_day -> :stop }
}

def PrintDay(day Day) (sig any) {
    println fmt.Println
    ---
    :day -> println -> :sig
}
