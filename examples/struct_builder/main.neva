import { fmt }

type User struct {
    age int
    name string
}

// FIXME:grammar - cannot use 'struct' as a node name

def Main(start any) (stop any) {
    println fmt.Println
    builder Struct<User>
    ---
    :start -> [
        'John' -> builder:name,
        32 -> builder:age
    ]
    builder -> println -> :stop
}
